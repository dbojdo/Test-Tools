{
    "name" : "webit/test-tools",
    "type": "library",
    "description": "Web-IT Test tools",
    "keywords": ["web-it", "testing", "unit test", "behaviour test"],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/symfony": "~2.4",
        "phpunit/phpunit": "~4.2",
        "behat/behat": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Tests\\" : "src"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "symfony/symfony": "To use standalone Symfony Bundle tools",
        "behat/behat": "To use Behat tools"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
