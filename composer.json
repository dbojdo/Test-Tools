{
    "name" : "webit/test-tools",
    "type": "library",
    "description": "Web-IT Test tools",
    "keywords": ["web-it", "testing", "unit test", "behaviour test"],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "symfony/http-kernel": "^3.0|^4.0",
        "phpunit/phpunit": "^5.0|^6.0",
        "behat/behat": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Tests\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\Tests\\" : "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "symfony/symfony": "To use standalone Symfony Bundle tools",
        "behat/behat": "To use Behat tools"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-0.9.x": "0.9.x-dev"
        }
    }
}
